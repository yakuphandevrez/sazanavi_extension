{
  "manifest_version": 3,
  "name": "AI PHISHING DETECTOR",
  "version": "1.0",
  "description": "URL güvenlik kontrolü",
  
  "permissions": [
    "storage"
  ],
  
  "host_permissions": [
    "https://safebrowsing.googleapis.com/*",
    "https://generativelanguage.googleapis.com/*"
  ],
  
  "action": {
    "default_popup": "popup.html"
  },
  
  "background": {
    "service_worker": "background.js"
  }
}